{
  "name": "labelmap-polydata",
  "version": "0.0.0-development",
  "license": "MIT",
  "type": "module",
  "description": "Convert vtk.js labelmap images to polygon meshes",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulHax/labelmap-polydata.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/labelmapToPolyDatas.js",
  "types": "dist/labelmapToPolyDatas.d.ts",
  "exports": {
    "./labelmapToPolyDatas": {
      "types": "./dist/labelmapToPolyDatas.d.ts",
      "import": "./dist/labelmapToPolyDatas.js"
    },
    "./serializeVtk": {
      "types": "./dist/serializeVtk.d.ts",
      "import": "./dist/serializeVtk.js"
    },
    "./directionMatrix": {
      "types": "./dist/directionMatrix.d.ts",
      "import": "./dist/directionMatrix.js"
    },
    "./workerHandler": {
      "types": "./dist/workerHandler.d.ts",
      "import": "./dist/workerHandler.js"
    },
    "./polyDataViewer": {
      "types": "./dist/polyDataViewer.d.ts",
      "import": "./dist/polyDataViewer.js"
    }
  },
  "scripts": {
    "dev": "vite --config vite.demo.config.ts",
    "build": "tsup",
    "build:demo": "vite build --config vite.demo.config.ts --outDir demo-dist",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@kitware/vtk.js": ">=30.0.0"
  },
  "devDependencies": {
    "@kitware/vtk.js": "^32.5.0",
    "semantic-release": "^25.0.2",
    "tsup": "^8.5.1",
    "typescript": "^5.6.0",
    "vite": "^6.0.0"
  }
}
