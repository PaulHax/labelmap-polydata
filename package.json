{
  "name": "labelmap-polydata",
  "version": "0.0.0-development",
  "license": "MIT",
  "type": "module",
  "description": "Convert vtk.js labelmap images to polygon meshes",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulHax/labelmap-polydata.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/labelmapToPolyDatas.js",
  "types": "dist/labelmapToPolyDatas.d.ts",
  "exports": {
    ".": {
      "import": "./dist/labelmapToPolyDatas.js",
      "types": "./dist/labelmapToPolyDatas.d.ts"
    },
    "./worker": {
      "import": "./src/worker.ts",
      "types": "./dist/worker.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "build:demo": "vite build --config vite.demo.config.ts --outDir demo-dist",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@kitware/vtk.js": ">=30.0.0"
  },
  "devDependencies": {
    "@kitware/vtk.js": "^32.5.0",
    "semantic-release": "^25.0.2",
    "typescript": "^5.6.0",
    "vite": "^6.0.0"
  }
}
